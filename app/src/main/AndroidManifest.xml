<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.canteenmanagment">

    <application
        android:allowBackup="true"
        android:icon="@drawable/foodlogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".ui.UserOrder.UserOrder" />
        <activity android:name=".ui.CartFoodList.CartFoodList" />
        <activity android:name=".SignUp_activity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/loginpage" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.FoodList.FoodListActivity" />
        <activity android:name=".ui.HomeActivity" />

        <service android:name=".utils.FirebaseNotification">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>